{
    "source": {
        "include": ["controllers", "models", "public", "utility", "viewmodels"],
        "exclude": ["public/js/google-analytics.js", 
            "public/js/d3.js", 
            "public/js/d3.min.js", 
            "public/js/ga.js", 
            "public/js/feedback.js",
            "public/js/html2canvas.js", 
            "public/js/jquery-2.1.4.js", 
            "public/js/loader.js", 
            "public/js/malarkey.js", 
            "public/js/malarkey.min.js", 
            "public/js/queue.v1.min.js", 
            "public/js/select.js", 
            "public/js/typeahead.js",
            "public/js/npm.js",
            "models/mongo",
            "models/mysql"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "template": "templates/default",  // same as -t templates/default
        "encoding": "utf8",               // same as -e utf8
        "destination": "./docs/",         // same as -d ./out/
        "recurse": true                   // same as -r
    }
}